<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity PostFavorite.
    -->
    <changeSet id="20191208091623-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="posting_item_id"
                                 baseTableName="post_favorite"
                                 constraintName="fk_post_favorite_posting_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="posting_item"/>

        <addForeignKeyConstraint baseColumnNames="user_profile_id"
                                 baseTableName="post_favorite"
                                 constraintName="fk_post_favorite_user_profile_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_profile"/>

    </changeSet>
</databaseChangeLog>
